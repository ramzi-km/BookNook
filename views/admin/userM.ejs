                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title"></h5>
                                <div class="table-responsive">
                                    <table id="zero_config" class="table table-striped table-bordered">
                                        <thead class="table-dark">
                                            <tr>
                                                <th class="text-white">N.O</th>
                                                <th class="text-white">Name</th>
                                                <th class="text-white">Email</th>
                                                <th class="text-white">mobile</th>
                                                <th class="text-white">Options</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% users.forEach((user,index)=> { %>
                                                <tr>
                                                    <td>
                                                        <%= index+1 %>
                                                    </td>
                                                    <td>
                                                        <%= user.name %>
                                                    </td>
                                                    <td>
                                                        <%= user.email %>
                                                    </td>
                                                    <td>
                                                        <%= user.mobile %>
                                                    </td>
                                                    <td>
                                                        <% if (user.block) { %>
                                                            <a href="/admin/userM/<%= user._id %> " class="btn btn-light btn-outline-secondary"
                                                                onclick="return confirm('Do you want to unblock <%= user.name %> ?')"><i
                                                                    class="fa fa-unlock-alt text-success"></i> Unblock</a>
                                                         <% } else { %>
                                                            <a href="/admin/userM/<%= user._id %> " class="btn btn-light btn-outline-dark"
                                                                onclick="return confirm('Do you really want to Block <%= user.name %> ?')"><i
                                                                    class="fa fa-ban text-danger"></i> Block</a>    
                                                         <% } %>



                                                    </td>
                                                </tr>
                                                <% }) %>

                                        </tbody>

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
